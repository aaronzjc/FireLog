{
  "manifest_version": 2,
  "name": "FireLog",
  "version": "1.0.2",
  "description": "FirePHP logs Viewer.",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": ["js/background.js"]
  },
  "devtools_page": "devtools.html",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"]
}
